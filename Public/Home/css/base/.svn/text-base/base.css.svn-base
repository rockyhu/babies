    .btn {
        font-size: 15px;
    }
    .container {
        width: 640px;
        margin-left: auto;
        margin-right: auto;
        padding-left: 20px;
        padding-right: 20px;
    }
    .pop-list {
        position: absolute;
        width: 600px;
        background-color: white;
        border-radius: 10px;
        left: 50%;
        bottom: 60px;
        margin-left: -300px;
    }
    .pop-enter-btn {
        border: none;
        position: absolute;
        background-color: #d0021b;
        width: 600px;
        height: 40px;
        margin-left: -300px;
        line-height: 40px;
        border-radius: 10px;
        color: white;
        left: 50%;
        bottom: 10px;
    }
}

@media screen and (max-width: 640px) {
    .header,
    .footer {
        width: 100%;
    }
    body,
    .form-control,
    .btn {
        font-size: 15px;
    }
    .container {
        width: 100%;
        margin-left: auto;
        margin-right: auto;
        padding-left: 20px;
        padding-right: 20px;
    }
    .pop-list {
        position: absolute;
        width: 92%;
        background-color: white;
        border-radius: 10px;
        bottom: 56px;
        margin-left: 4%;
    }
    .pop-enter-btn {
        border: none;
        position: absolute;
        background-color: #d0021b;
        width: 92%;
        height: 40px;
        margin-left: 4%;
        line-height: 40px;
        border-radius: 10px;
        color: white;
        bottom: 8px;
    }
}


