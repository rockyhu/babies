@CHARSET "UTF-8";
/*@import "http://fonts.useso.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic";*/
* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

html,
body,
div,
p,
ul,
li,
dl,
dt,
dd,
h1,
h2,
h3,
h4,
h5,
h6,
form,
input,
button,
textarea,
iframe,
table,
th,
td {
    margin: 0;
    padding: 0;
    outline: none;
    font-family: "Source Sans Pro","微软雅黑","Helvetica Neue",Helvetica,Arial,sans-serif;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-appearance: none;
    /*去掉移动版Safari浏览器中阴影*/
    -moz-appearance: none;
    appearance: none;
}

img {
    border: 0 none;
    vertical-align: top;
}

body {
    font-weight: normal;
    background-color: #f7f7ef;
    font-size: 14px;
    -moz-user-select: none;
    /*火狐*/
    -webkit-user-select: none;
    /*webkit浏览器*/
    -ms-user-select: none;
    /*IE10*/
    -khtml-user-select: none;
    /*早期浏览器*/
    user-select: none;
    /*渐变背景实现方式*/
    /*background-attachment: fixed, fixed;
    background-color: rgba(45, 101, 203, 0.89);
    background-image: radial-gradient(ellipse farthest-side at 10% 0 , rgba(119, 157, 207, 0.8) 30%, rgba(174, 180, 186, 0.8) 76%, rgba(199, 175, 156, 0.8));
    color: #f8f8f8;
    overflow-y: scroll;*/
}

ol,
ul,
li {
    list-style: none
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-weight: inherit;
}

button,
input[type=button],
input[type=reset],
input[type=submit] {
    -webkit-appearance: button;
    cursor: pointer;
    outline: none;
    text-transform: none;
    margin: 0;
    font: inherit;
    color: inherit;
}

i,
em,
cite {
    font-style: normal;
}


/*屏蔽点击元素出现底色*/

a {
    -webkit-tap-highlight-color: rgba(255, 255, 255, 0);
    color: #555;
}

a:focus {
    outline: none;
}


/*屏蔽点击元素出现底色*/

.tap-bg {
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

a:link,
a:visited {
    text-decoration: none;
    outline: none;
}

a:hover,
a:ctive {
    text-decoration: none;
    border: none;
}

.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}


/* 清除浮动*/

.clearfix {
    zoom: 1
}


/* Hides from IE-mac 兼容IE*/

*html .clearfix {
    height: 1%;
}

.header {
    height: 45px;
    line-height: 45px;
    text-align: center;
    background-color: #d32f2f;
    color: #fff;
    font-size: 16px;
    margin-left: auto;
    margin-right: auto;
    position: fixed;
    top: 0;
    z-index: 9999;
}

.header .backbtn {
    width: 26px;
    height: 26px;
    line-height: 26px;
    text-align: center;
    border: 1px solid #fff;
    border-radius: 18px;
    position: absolute;
    top: 9px;
    left: 10px;
}

.header .backbtn a {
    margin-top: -1px;
    margin-left: -2px;
    width: 26px;
    height: 26px;
    display: block;
    color: #fff;
    font-size: 16px;
}

span.red{
    color: #d0021b;
}

.header .action {
    position: absolute;
    top: 0;
    right: 10px;
}

.header .action a {
    font-size: 24px;
    color: #fff;
}

.container .panel {
    padding-top: 30px;
}

.form-group {
    margin-bottom: 3px;
    position: relative;
}

.form-control {
    display: block;
    width: 100%;
    height: 45px;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    border: 1px solid #fff;
    border-radius: 0;
}

.form-group:first-child input.form-control {
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

.form-group:last-child input.form-control {
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
}

/*表单验证成功样式*/
input.form-success,
textarea.form-success{
    border-color: #fff;
}

/*表单验证失败样式*/
input.form-error,
textarea.form-error{
    border-color: #d32f2f !important;
}

.btn {
    display: inline-block;
    width: 100%;
    height: 45px;
    padding: 6px 12px;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: 400;
    line-height: 1.42857143;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 5px;
    color: #fff;
    letter-spacing: 2px;
}

.btn-primary {
    background-color: #d32f2f;
    color: #fff;
}

/*按钮禁用样式*/
.btn.disabled,
.btn[disabled],
fieldset[disabled] .btn {
    box-shadow: none;
    cursor: not-allowed;
    opacity: 0.65;
    pointer-events: none;
}

.footer {
    position: absolute;
    color: #d32f2f;
}

.center {
    text-align: center;
    margin-left: auto;
    margin-right: auto;
}

.footer .center a {
    color: #d32f2f;
}

.pull-left {
    float: left;
}

.pull-right {
    float: right;
}

.hidetext {
    text-indent: -9999px;
}

.hide {
    display: none;
}


/** 通用弹窗 start**/

.pop-mask {
    display: none;
    position: fixed;
    z-index: 999;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background-color: rgba(0, 0, 0, 0.34);
    backface-visibility: hidden;
}

.pop-item {
    height: 40px;
    line-height: 40px;
    color: #d0021b;
    font-size: 14px;
    text-align: center;
    /*border-bottom: solid 1px rgba(203, 204, 190, 0.8);*/
    position:relative;
}

.pop-item::before{
    border-top: 1px solid #ececec;
    content: " ";
    height: 1px;
    left: 0;
    position: absolute;
    top: 0;
    transform: scaleY(0.5);
    transform-origin: 0 0 0;
    width: 100%;
}

.weui_actionsheet_toggle {
    transform: translate(0px, 0px);
}

.pop-item.hover
.pop-item:active {
	background-color:rgba(0, 0, 0, 0.2);
	-webkit-tap-highlight-color:transparent;
}

.pop-item:first-child.hover{
	border-top-left-radius:10px;
	border-top-right-radius:10px;
}

.pop-item:last-child.hover{
	border-bottom-left-radius:10px;
	border-bottom-right-radius:10px;
}

.pop-item:first-child::before {
    display: none;
}

.pop-item .icon{
	display:none;
	width:30px;
}

.selected .icon {
	display:inline;
}

/** 通用弹窗 end **/


/*下拉菜单公共样式 start */

.select {
    position: relative;
}

.select .select-down {
    width: 100%;
    background-color: rgba(0, 0, 0, 0.8);
    position: absolute;
    left: 0;
}

.select .select-down li {
    font-size: 14px;
    height: 38px;
    line-height: 38px;
    border-bottom: 1px solid #999;
    color: #fff;
    text-align: center;
    cursor: pointer;
}
.select .select-down li:last-child{
    border-bottom:none;
}
.select .select-down li a{
    color: #fff;
}
.select .select-down li i{
    display: none;
    /*color: #d32f2f;*/
}

.select .select-down li.selected{
    background-color: rgba(211,47,47,0.8);
}
.select .select-down li.selected i{
    display: inline-block;
}

/*下拉菜单公共样式 end */


