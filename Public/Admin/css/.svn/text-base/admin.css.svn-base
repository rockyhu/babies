/*批量上传图片相关样式*/
.fileupload .weibo_pic_list{
	/*padding:15px 10px 10px 0;
	display:none;*/
	padding-top:15px;
}
.fileupload .weibo_pic_content{
	width:150px;
	height:150px;
	border:1px solid #eee;
	float:left;
	margin:-5px 4px 15px 4px;
	overflow:hidden;
	position:relative;
}
.fileupload .weibo_pic_content .remove{
	display:none;
	width:150px;
	height:25px;
	background:#000;
	position:absolute;
	top:125px;
	left:0;
	z-index:2;
	opacity:0.7;
	filter:alpha(opacity=70);
	cursor:pointer;
}
.fileupload .weibo_pic_content .text{
	display:none;
	width:150px;
	height:25px;
	line-height:25px;
	text-align:center;
	position:absolute;
	top:125px;
	left:0;
	z-index:3;
	cursor:pointer;
	color:#fff;
	font-size:13px;
}
.fileupload img.weibo_pic_img{
	position:absolute;
	top:0;
	left:0;
	z-index:1;
	cursor:move;
	width:150px;
}
.fileupload .sort-highlight{
	background: #f4f4f4;
	border: 1px dashed #ddd;
	width:150px;
	height:150px;
	float:left;
	margin:-5px 4px 15px 4px;
	overflow:hidden;
	position:relative;
}
